:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5ce018eea250 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5ce018eea3e0 .scope module, "mac_unit_tb" "mac_unit_tb" 3 4;
 .timescale -9 -12;
v0x5ce018f07e60_0 .var/s "a", 7 0;
v0x5ce018f07f40_0 .net/s "acc", 31 0, L_0x5ce018f08470;  1 drivers
v0x5ce018f08010_0 .var/s "b", 7 0;
v0x5ce018f08110_0 .var "clear_acc", 0 0;
v0x5ce018f081e0_0 .var "clk", 0 0;
v0x5ce018f082d0_0 .var "enable", 0 0;
v0x5ce018f083a0_0 .var "rst", 0 0;
S_0x5ce018eae490 .scope module, "dut" "mac_unit" 3 14, 4 5 0, S_0x5ce018eea3e0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /INPUT 1 "clear_acc";
    .port_info 4 /INPUT 8 "a";
    .port_info 5 /INPUT 8 "b";
    .port_info 6 /OUTPUT 32 "acc";
L_0x5ce018f08470 .functor BUFZ 32, v0x5ce018f07890_0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x5ce018eae720_0 .net/s "a", 7 0, v0x5ce018f07e60_0;  1 drivers
v0x5ce018f077b0_0 .net/s "acc", 31 0, L_0x5ce018f08470;  alias, 1 drivers
v0x5ce018f07890_0 .var/s "accumulator", 31 0;
v0x5ce018f07950_0 .net/s "b", 7 0, v0x5ce018f08010_0;  1 drivers
v0x5ce018f07a30_0 .net "clear_acc", 0 0, v0x5ce018f08110_0;  1 drivers
v0x5ce018f07b40_0 .net "clk", 0 0, v0x5ce018f081e0_0;  1 drivers
v0x5ce018f07c00_0 .net "enable", 0 0, v0x5ce018f082d0_0;  1 drivers
v0x5ce018f07cc0_0 .net "rst", 0 0, v0x5ce018f083a0_0;  1 drivers
E_0x5ce018eaeb40 .event posedge, v0x5ce018f07cc0_0, v0x5ce018f07b40_0;
    .scope S_0x5ce018eae490;
T_0 ;
    %wait E_0x5ce018eaeb40;
    %load/vec4 v0x5ce018f07cc0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5ce018f07890_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ce018f07a30_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5ce018f07890_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5ce018f07c00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x5ce018f07890_0;
    %load/vec4 v0x5ce018eae720_0;
    %pad/s 32;
    %load/vec4 v0x5ce018f07950_0;
    %pad/s 32;
    %mul;
    %add;
    %assign/vec4 v0x5ce018f07890_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5ce018eea3e0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ce018f081e0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5ce018eea3e0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x5ce018f081e0_0;
    %inv;
    %store/vec4 v0x5ce018f081e0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5ce018eea3e0;
T_3 ;
    %vpi_call/w 3 30 "$display", "[MAC TB] Starting test..." {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ce018f083a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ce018f082d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ce018f08110_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5ce018f07e60_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5ce018f08010_0, 0, 8;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ce018f083a0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call/w 3 44 "$display", "[TEST 1] MAC operations: 2*3 + 4*5 + 1*1" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ce018f082d0_0, 0, 1;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x5ce018f07e60_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x5ce018f08010_0, 0, 8;
    %delay 10000, 0;
    %vpi_call/w 3 49 "$display", "  After 2*3: acc = %0d (expected 6)", v0x5ce018f07f40_0 {0 0 0};
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x5ce018f07e60_0, 0, 8;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x5ce018f08010_0, 0, 8;
    %delay 10000, 0;
    %vpi_call/w 3 53 "$display", "  After 4*5: acc = %0d (expected 26)", v0x5ce018f07f40_0 {0 0 0};
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x5ce018f07e60_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x5ce018f08010_0, 0, 8;
    %delay 10000, 0;
    %vpi_call/w 3 57 "$display", "  After 1*1: acc = %0d (expected 27)", v0x5ce018f07f40_0 {0 0 0};
    %vpi_call/w 3 60 "$display", "[TEST 2] Clear accumulator" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ce018f08110_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ce018f08110_0, 0, 1;
    %vpi_call/w 3 64 "$display", "  After clear: acc = %0d (expected 0)", v0x5ce018f07f40_0 {0 0 0};
    %vpi_call/w 3 67 "$display", "[TEST 3] Negative: (-5)*3 + 2*(-4) = -15 + -8 = -23" {0 0 0};
    %pushi/vec4 251, 0, 8;
    %store/vec4 v0x5ce018f07e60_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x5ce018f08010_0, 0, 8;
    %delay 10000, 0;
    %vpi_call/w 3 70 "$display", "  After (-5)*3: acc = %0d (expected -15)", v0x5ce018f07f40_0 {0 0 0};
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x5ce018f07e60_0, 0, 8;
    %pushi/vec4 252, 0, 8;
    %store/vec4 v0x5ce018f08010_0, 0, 8;
    %delay 10000, 0;
    %vpi_call/w 3 74 "$display", "  After 2*(-4): acc = %0d (expected -23)", v0x5ce018f07f40_0 {0 0 0};
    %delay 20000, 0;
    %vpi_call/w 3 78 "$display", "[MAC TB] All tests complete!" {0 0 0};
    %vpi_call/w 3 79 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5ce018eea3e0;
T_4 ;
    %vpi_call/w 3 84 "$dumpfile", "KAB/sim/waves/mac_unit.vcd" {0 0 0};
    %vpi_call/w 3 85 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5ce018eea3e0 {0 0 0};
    %end;
    .thread T_4;
